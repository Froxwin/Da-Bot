{
  "name": "da-bot",
  "author": "Froxwin",
  "version": "1.0.0",
  "description": "it's a bot",
  "license": "MIT",
  "type": "module",
  "main": "./out/index",
  "scripts": {
    "clean": "if exist out (rmdir /s /q out)",
    "build": "tsc -b",
    "make": "make.ps1",
    "start": "node --enable-source-maps .",
    "dev": "npm run clean && npm run make && npm run start",
    "lint": "npx prettier -c ./src && npx eslint ./src --ext .ts",
    "format": "npx prettier -w . && npx eslint . --ext .ts --fix"
  },
  "prettier": {
    "printWidth": 75,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "indent": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Froxwin/Da-Bot"
  },
  "dependencies": {
    "discord.js": "^14.7.1",
    "random-anime": ">=1.0.6"
  },
  "devDependencies": {
    "@types/node": ">=18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": ">=4.9.4"
  },
  "engines": {
    "node": ">=19.1.0"
  }
}
